<Window x:Class="Laba1.Third"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Laba1"
        mc:Ignorable="d"
        Title="Third" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBox Text="Input the precision" Width="200" TextAlignment="Center"/>
            <TextBox x:Name="precision" 
                     TextWrapping="Wrap" 
                     AcceptsReturn="True"
                     VerticalAlignment="Stretch" 
                     HorizontalAlignment="Stretch" 
                     Margin="10"
                     Width="100"
                     Height="20"/>
            <Button Name="Button" Content="Submit" Click="submit_button" Width="100"></Button>
            <StackPanel HorizontalAlignment="Center" Margin="10,10,10,10">
                <TextBlock Name="one"/>
                <TextBlock Name="two"/>
                <TextBlock Name="three"/>
            </StackPanel>
        </StackPanel>
        
    </Grid>
</Window>
